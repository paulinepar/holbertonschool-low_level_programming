#include "main.h"

/**
 * print_square - entry point
 * @n: check character
 * Return: Always 0 (Success)
 */

void print_square(int size)
{
	int a, b;
	if (size <= 0)
		_putchar(10);
	else
	{
		for (i = 0; i < size; i++)
		{
			for (n = 0; n < size; n++)
			{
				_putchar('#');
			}
			_putchar('\n');
		}
	}
}
