#include "main.h"

/**
 * pop_listint - delete the head note
 * @head: double pointer
 * Return: n
 */

unsigned int binary_to_uint(const char *b)
{
	int n = 0;
	unsigned int i = 0;

	if (b == NULL)
		return (0);

	while (b[n + 1])
	{
		if (b[n] != '0' || b[n] != '1')
		{
			return (0);
			n++;
		}
	}
	return (i);
}
