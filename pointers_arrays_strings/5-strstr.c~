#include "main.h"
#include <stdio.h>

/**
 * _strstr - locates substring
 * @haystack: string
 * @needle: string
 * Return: 0
 */
char *_strstr(char *haystack, char *needle)
{
	int i, a;

	for ( i = 0; haystack[i] != '\0'; i++)
	{
		for (a = 0; needle[a] != '\0'; a++)
		{
			if ((haystack[i + a]) = needle[a])
				return ((needle + a);
		}
	}
	return (0);
}
