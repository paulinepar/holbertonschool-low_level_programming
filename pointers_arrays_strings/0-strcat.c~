#include "main.h"

/**
 * _strcpy - print a string
 * @src: char
 * @dest: char
 * Return: (i)
 */

char *_strcat(char *dest, char *src)
{
	int n;
	int i;
	
	for (n = 0; dest[n] != '\0'; n++)
	{
	}
	for (i = 0; src[i] != '\0'; i++)
		{
			dest[n + i] = src[i];
		}
	dest[n + i] = '\0';
	return (dest);
}
